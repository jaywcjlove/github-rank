    "@types/node-fetch": "2.5.12",
    "@uiw/formatter": "1.2.4",
    "cheerio": "1.0.0-rc.10",
    "console-emojis": "1.0.4",
    "dotenv": "10.0.0",
    "ejs": "3.1.6",
    "fs-extra": "10.1.0",
    "node-fetch": "2.6.5",
