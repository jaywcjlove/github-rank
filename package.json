    "get:users:china": "node lib/getUsersChina.js",
    "get:repos": "node lib/getRepos.js",
    "get:trending": "node lib/getTrending.js",
    "prepare": "npm run build",
    "dev": "npm run watch",
    "watch": "tsbb watch --no-esm --disable-babel-option",
